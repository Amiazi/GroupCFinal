<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">


<suite name="StateFarm Test Suite" parallel="false">

    <parameter name="useCloudEnv" value="false"/>
    <parameter name="cloudEnvName" value="browserstack"/>
    <!--<parameter name="cloudEnvName" value="saucelab"/>-->
    <parameter name="os" value="windows"/>
    <!--    <parameter name="os" value="OS X"/>-->
    <parameter name="os_version" value="11"/>
    <!--        <parameter name="os_version" value="Sierra"/>-->
    <parameter name="browserName" value="chrome"/>
    <parameter name="browserVersion" value="111"/>
    <parameter name="url" value="https://www.statefarm.com/"/>

    <test name="HomePage">
        <classes>
            <class name="Statefarm.TestSFHelpButton"></class>
            <class name="Statefarm.TestFindInsurance"></class>
            <class name="Statefarm.TestStartAndEndChat"></class>
            <class name="Statefarm.TestAgentBttn"></class>
            <class name="Statefarm.TestTrackClaimButton"></class>
            <class name="Statefarm.TestPayBill"></class>
            <class name="Statefarm.TestSearchFunction"></class>
            <class name="Statefarm.TestInvalidLogin"></class>

        </classes>

    </test>

</suite>